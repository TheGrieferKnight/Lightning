# .env.example
# Copy this file to .env and fill in your actual values

# Riot Games API Key (get from https://developer.riotgames.com/)
REACT_APP_RIOT_API_KEY=your_riot_api_key_here

# Your backend API base URL (if you have a backend proxy)
REACT_APP_API_BASE_URL=http://localhost:3001/api

# Default region
REACT_APP_DEFAULT_REGION=na1

# Enable/disable mock data (set to 'false' when using real API)
REACT_APP_USE_MOCK_DATA=true